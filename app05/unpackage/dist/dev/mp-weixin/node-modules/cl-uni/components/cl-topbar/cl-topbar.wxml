<view class="{{['cl-topbar',classList]}}" style="{{'padding-top:'+(top)+';'+('background-color:'+(backgroundColor)+';')+('color:'+(color)+';')+('z-index:'+(zIndex)+';')}}"><view class="cl-topbar__content" style="{{'color:'+(color)+';'}}"><block wx:if="{{$slots.default}}"><slot></slot></block><block wx:else><view data-event-opts="{{[['tap',[['tapText',['$event']]]]]}}" class="cl-topbar__text" bindtap="__e"><block wx:if="{{title}}"><text class="cl-topbar__title">{{title}}</text></block><block wx:if="{{description}}"><text class="cl-topbar__description">{{description}}</text></block></view></block></view><view class="cl-topbar__prepend" style="{{'top:'+(top)+';'}}"><block wx:if="{{showBack}}"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="cl-topbar__icon" bindtap="__e"><text class="cl-icon-arrow-left"></text></view></block><slot name="prepend"></slot></view><view class="cl-topbar__append" style="{{'top:'+(top)+';'}}"><slot name="append"></slot></view></view>