<view class="{{['cl-tabs',classList]}}"><view class="cl-tabs__header" style="{{'top:'+(stickyTop)+';'}}"><scroll-view class="cl-tabs__bar" style="{{$root.s0}}" scroll-with-animation="{{true}}" scroll-x="{{true}}" scroll-left="{{scrollLeft}}"><view class="cl-tabs__bar-box" style="{{'justify-content:'+(justify)+';'}}"><block wx:for="{{tabs}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['change',[index]]]]]}}" class="{{['cl-tabs__bar-item',(value===item.name)?'is-active':'']}}" style="{{'color:'+(value===item.name?color:unColor)+';'+('padding:'+('0 '+gutter+'rpx')+';')}}" bindtap="__e"><block wx:if="{{item.prefixIcon}}"><text class="{{['cl-tabs__icon','cl-tabs__icon--prefix',item.prefixIcon]}}"></text></block><text class="cl-tabs__label">{{item.label}}</text><block wx:if="{{item.suffixIcon}}"><text class="{{['cl-tabs__icon','cl-tabs__icon--suffix',item.suffixIcon]}}"></text></block></view></block><block wx:if="{{lineLeft>0}}"><view class="cl-tabs__line" style="{{'background-color:'+(color)+';'+('left:'+(lineLeft+'px')+';')}}"></view></block></view></scroll-view><block wx:if="{{showDropdown}}"><view data-event-opts="{{[['tap',[['onDropdown',['$event']]]]]}}" class="cl-tabs__dropdown" style="{{$root.s1}}" bindtap="__e"><cl-icon vue-id="9ba512a8-1" name="{{''+(dropdown.visible?'arrow-top':'arrow-bottom')}}" bind:__l="__l"></cl-icon></view></block><view class="cl-tabs__dropdown-box" style="{{'max-height:'+(dropdown.visible?dropdown.height:'0')+';'}}"><slot name="dropdown"></slot></view></view><view data-ref="pane" data-event-opts="{{[['touchstart',[['onTouchStart',['$event']]]],['touchend',[['onTouchEnd',['$event']]]]]}}" class="cl-tabs__container vue-ref" bindtouchstart="__e" bindtouchend="__e"><slot></slot></view></view>