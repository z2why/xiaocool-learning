.cl-input {
  display: flex;
  height: 70rpx;
  width: 100%;
  vertical-align: middle;
  position: relative;
  border-radius: 8rpx;
  box-sizing: border-box;
  transition: border-color 0.2s;
  padding: 0 20rpx;
  background-color: #fff;
}
.cl-input--focus {
  border-color: rgba(64, 158, 255, 0.7);
}
.cl-input__wrap {
  display: flex;
  align-items: center;
  flex: 1;
  height: 100%;
  position: relative;
}
.cl-input__inner {
  height: 100%;
  flex: 1;
  font-size: 26rpx;
  border: 0;
  padding: 0;
  outline: none;
  background-color: transparent;
}
.cl-input__prepend, .cl-input__append {
  display: flex;
  align-items: center;
  font-size: 26rpx;
}
.cl-input__prepend {
  margin-right: 20rpx;
}
.cl-input__append {
  margin-left: 20rpx;
}
.cl-input__clear, .cl-input__icon {
  display: inline-block;
  font-size: 34rpx;
  color: #999;
  cursor: pointer;
}
.cl-input__clear {
  color: #c1c1c1;
  margin-left: 20rpx;
}
.cl-input:not(.is-disabled) {
  -webkit-box-shadow: 0 0 0 1000px transparent inset;
}
.cl-input--prefix, .cl-input--suffix {
  flex-shrink: 0;
}
.cl-input--prefix .cl-input__icon, .cl-input--suffix .cl-input__icon {
  position: absolute;
  text-align: center;
}
.cl-input--prefix .cl-input__wrap {
